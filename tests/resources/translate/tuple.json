{
  "name": "tuple",
  "tests": [
    {
      "name": "test_tuple_atomic",
      "compatible": true,
      "test": {
        "avro": {
          "fields": [
            {
              "default": "null",
              "name": "_f0",
              "type": [
                {
                  "type": "null"
                },
                {
                  "type": "boolean"
                }
              ]
            },
            {
              "default": "null",
              "name": "_f1",
              "type": [
                {
                  "type": "null"
                },
                {
                  "type": "string"
                }
              ]
            }
          ],
          "name": "root",
          "type": "record"
        },
        "bigquery": [
          {
            "mode": "NULLABLE",
            "name": "_f0",
            "type": "BOOL"
          },
          {
            "mode": "NULLABLE",
            "name": "_f1",
            "type": "STRING"
          }
        ],
        "json": {
          "additionalItems": false,
          "items": [
            {
              "type": "boolean"
            },
            {
              "type": "string"
            }
          ],
          "type": "array"
        }
      }
    },
    {
      "description": [
        "Additional items in the tuple requires setting the max number of items"
      ],
      "name": "test_tuple_atomic_with_additional_items",
      "compatible": true,
      "test": {
        "avro": {
          "fields": [
            {
              "default": "null",
              "name": "_f0",
              "type": [
                {
                  "type": "null"
                },
                {
                  "type": "boolean"
                }
              ]
            },
            {
              "default": "null",
              "name": "_f1",
              "type": [
                {
                  "type": "null"
                },
                {
                  "type": "string"
                }
              ]
            },
            {
              "default": "null",
              "name": "_f2",
              "type": [
                {
                  "type": "null"
                },
                {
                  "type": "long"
                }
              ]
            },
            {
              "default": "null",
              "name": "_f3",
              "type": [
                {
                  "type": "null"
                },
                {
                  "type": "long"
                }
              ]
            }
          ],
          "name": "root",
          "type": "record"
        },
        "bigquery": [
          {
            "mode": "NULLABLE",
            "name": "_f0",
            "type": "BOOLEAN"
          },
          {
            "mode": "NULLABLE",
            "name": "_f1",
            "type": "STRING"
          },
          {
            "mode": "NULLABLE",
            "name": "_f2",
            "type": "INT64"
          },
          {
            "mode": "NULLABLE",
            "name": "_f3",
            "type": "INT64"
          }
        ],
        "json": {
          "additionalItems": {
            "type": "integer"
          },
          "items": [
            {
              "type": "boolean"
            },
            {
              "type": "string"
            }
          ],
          "maxItems": 4,
          "type": "array"
        }
      }
    }
  ]
}
